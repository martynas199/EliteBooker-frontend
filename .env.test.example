# Frontend Test Environment Variables

# API URL
VITE_API_URL=http://localhost:5000

# Stripe Public Key (Test Mode)
VITE_STRIPE_PUBLIC_KEY=pk_test_your_stripe_test_public_key

# Google OAuth (Optional)
VITE_GOOGLE_CLIENT_ID=your_google_client_id

# Google Maps API Key (for search page)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Default location for search page (fallback when geolocation is unavailable)
# London coordinates as default:
VITE_DEFAULT_LAT=51.5074
VITE_DEFAULT_LNG=-0.1278

# Analytics (Disabled in test)
VITE_ANALYTICS_ENABLED=false

# Environment
VITE_ENV=test

# Sentry
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=test
VITE_SENTRY_SEND_DEFAULT_PII=true
VITE_SENTRY_ENABLE_LOGS=true
VITE_SENTRY_TRACES_SAMPLE_RATE=1.0
VITE_SENTRY_REPLAYS_SESSION_SAMPLE_RATE=1.0
VITE_SENTRY_REPLAYS_ON_ERROR_SAMPLE_RATE=1.0
